import { OnInit, EventEmitter, ElementRef } from '@angular/core';
export declare class TextComponent implements OnInit {
    private elementRef;
    type: any;
    maxlength: any;
    showMask: any;
    value: any;
    name: any;
    maskType: any;
    patterns: any;
    expression: any;
    label: string;
    fixValue: string;
    switchStyle: boolean;
    signalStyle: boolean;
    staticStyle: boolean;
    style: any;
    borderStyle: any;
    valueStyle: any;
    inputStyle: any;
    readOnly: boolean;
    onlyText: boolean;
    noPutCharacter: number;
    getvalue: EventEmitter<any>;
    onblur: EventEmitter<any>;
    constructor(elementRef: ElementRef);
    ngOnInit(): void;
    ngOnChanges(event: any): void;
    getValue(event: any): void;
    onBlur(event: any): void;
    onEndfocus(event: any): void;
    keyPress(e: any): void;
}

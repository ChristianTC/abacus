import { OnInit, EventEmitter } from '@angular/core';
export declare class PaginationComponent implements OnInit {
    totalRecords: number;
    noPage: number;
    pageSize: number;
    clickPage: EventEmitter<any>;
    noItems: number[];
    nItem: number;
    paginators: number;
    constructor();
    ngOnInit(): void;
    setPaginator(n: any): void;
    changePage(n: any): void;
}
